    <style>
body,html{width:100%;height:100%;overflow:hidden}canvas{position:absolute;top:0;left:0}
article,aside,details,figcaption,figure,footer,header,hgroup,main,menu,nav,section,time{display:block}iframe{border:none}ul{list-style-type:none}

html,body{margin:0;padding: 0;height:100%;font:14px/1.5 "Microsoft YaHei", "微软雅黑";}
a{text-decoration:none;}
.error { width:566px;height:600px;margin:0 auto;text-align: center;padding-top: 130px; }

.step2-2 {
    position: absolute;
    bottom: 0;
    left: 0;
    display: none;
    overflow: hidden;
}
.p1 {
    position: relative;
    z-index: 1;
}
.p2 {
    position: absolute;
    left: 33px;
    top: 215px;
    -webkit-animation: zhuan2 0.4s linear infinite;
    animation: zhuan2 0.4s linear infinite;
    z-index: 1
}
.p3 {
    position: absolute;
    left: 353px;
    top: 212px;
    -webkit-animation: zhuan2 0.4s linear infinite;
    animation: zhuan2 0.4s linear infinite;
    z-index: 1
}
.p4 {
    position: absolute;
    left: 550px;
    top: 0;
    z-index: 2;
    transition: left 1s ease-out;
    -webkit-transition: left 1s ease-out;
    display: none;
}
.p5 {
    position: absolute;
    left: 0;
    top: 0;
    z-index: 0;
}

@media only screen and (max-width: 680px){
    .error { width:auto; padding-left: 20px; padding-right: 20px; }
    .error img{ width: 100%; }
}
::-moz-selection{color:#ffffff;background:#dc3333}::selection{color:#ffffff;background:#dc3333}

@-webkit-keyframes zhuan2{
    0%{
        -webkit-transform: rotate(0deg);
    }
    100%{
        -webkit-transform: rotate(360deg);
    }
}

@keyframes zhuan2{
    0%{
        -webkit-transform: rotate(0deg);
        transform: rotate(0deg);
    }
    100%{
        -webkit-transform: rotate(360deg);
        transform: rotate(360deg);
    }
}

